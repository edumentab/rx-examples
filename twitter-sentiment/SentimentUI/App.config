<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Net.Http.Primitives" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.2.18.0" newVersion="4.2.18.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <appSettings>
    <add key="consumerKey" value="YOUR_KEY_HERE" />
    <add key="consumerSecret" value="YOUR_SECRET_HERE" />
    <add key="accessToken" value="YOUR_TOKEN_HERE" />
    <add key="accessTokenSecret" value="YOUR_SECRET_HERE" />
  </appSettings>
</configuration>